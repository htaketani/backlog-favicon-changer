{
    "manifest_version": 2,
    "name": "Backlog Favicon Changer",
    "description": "Backlog Favicon Changer",
    "version": "0.3.0",
    "icons": {
        "128": "icon_128.png",
        "48": "icon_48.png",
        "16": "icon_16.png"
    },
    "permissions": [
        "tabs",
        "https://*.backlog.jp/*"
    ],
    "content_scripts": [
        {
            "matches": [ "https://*.backlog.jp/*" ],
            "js": [
                "js/jquery-2.1.3.min.js",
                "js/underscore-min.js",
                "js/script.js"
            ],
            "run_at": "document_end"
        }
    ]
}
